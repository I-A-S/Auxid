{
  "version": 4,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 28
  },
  "configurePresets": [
    {
      "name": "auxid-validator-x64-linux",
      "displayName": "Auxid Validator Linux x64 (Clang)",
      "inherits": "x64-linux",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/IACrux/cmake/toolchains//x64-linux-clang.cmake"
      }
    },
    {
      "name": "auxid-validator-arm64-linux",
      "displayName": "Auxid Validator Linux ARM64 (Clang Cross)",
      "inherits": "arm64-linux",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/IACrux/cmake/toolchains//arm64-linux-clang.cmake"
      }
    },
    {
      "name": "auxid-validator-x64-windows",
      "displayName": "Auxid Validator Windows x64 (Clang)",
      "inherits": "x64-windows",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/IACrux/cmake/toolchains//x64-windows-clang.cmake"
      }
    },
    {
      "name": "auxid-validator-arm64-windows",
      "displayName": "Auxid Validator Windows ARM64 (Clang Cross)",
      "inherits": "arm64-windows",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/IACrux/cmake/toolchains//arm64-windows-clang.cmake"
      }
    },
    {
      "name": "auxid-validator-x64-windows-cross",
      "displayName": "Auxid Validator Windows x64 (Clang Cross Linux Host)",
      "inherits": "x64-windows-cross",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/IACrux/cmake/toolchains//x64-windows-cross-clang.cmake"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "auxid-validator-x64-linux",
      "configurePreset": "auxid-validator-x64-linux"
    },
    {
      "name": "auxid-validator-x64-windows",
      "configurePreset": "auxid-validator-x64-windows"
    }
  ],
  "include": [
    "IACrux/CMakePresets.json"
  ]
}